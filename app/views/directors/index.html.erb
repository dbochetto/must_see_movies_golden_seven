<h1> Directors <h1>

<a href="http://localhost:3000/directors/new">Add a New Director</a>

<table>
  <tr>
    <th>Image</th>
    <th>Name</th>
    <th>Biography</th>
    <th>Date of Birth</th>
    <th colspan="3">Actions</th>
  </tr>
  <% @list_of_directors.each do |director| %>
  <tr>
    <td>
      <img src="<%= director.image_url %>" width="80">
    </td>
    <td>
      <%= director.name %>
    </td>
    <td>
      <%= director.bio %>
    </td>
    <td>
      <%= director.dob %>
    </td>
    <td>
      <a href="http://localhost:3000/directors/<%= director.id %>">Show</a>
    </td>
    <td>
      <a href="http://localhost:3000/directors/edit/<%= director.id %>">Edit</a>
    </td>
    <td>
      <a href="http://localhost:3000/directors/delete_director/<%= director.id %>">Delete</a>
    </td>
  </tr>
  <% end %>
</table>
